<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans.xsd 
  http://www.springframework.org/schema/context/ 
  http://www.springframework.org/schema/context/spring-context.xsd">
  <bean id  ="car" class = "com.chainsys.springproject.autowire.Car"></bean> 
  <bean id  ="eng" class = "com.chainsys.springproject.autowire.Wheel"></bean>
  <bean id  ="wh" class = "com.chainsys.springproject.autowire.Engine"></bean>
  <bean class="org.springframework.beans.factory.annotation.AutowiredAnnotationBeanPostProcessor"/>
   <bean id  ="cs" class = "com.chainsys.springproject.autowire.CarServices" scope = "prototype"></bean>
   
   <!-- <bean id  ="SuvCar" class = "com.chainsys.springproject.autowire.SuvCar" scope = "prototype"></bean> -->
   
   <bean id  ="SportsCar" class = "com.chainsys.springproject.autowire.SportsCar" scope = "prototype"></bean></beans>